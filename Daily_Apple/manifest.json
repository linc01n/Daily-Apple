{
  "name": "Daily Apple",
  "manifest_version": 2,
  "version": "2.0.0",
  "description": "Load Apple Daily News in one page",
  "icons":
  {
	"16": "icon_16.png",
	"48": "icon_48.png",
	"128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://hk.apple.nextmedia.com/*/index/"],
      "js": ["lib/underscore.1.4.3-min.js", "lib/mootools-1.4.5-min.js","da.js"]
    }
  ],
  "permissions": [
	"http://hk.apple.nextmedia.com/*/index/"
  ]
}
